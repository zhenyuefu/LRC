    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Prolog Files",
            "type": "prolog",
            "request": "launch",
            "runtimeExecutable": "/opt/homebrew/bin/swipl",
            "runtimeArgs": [],
            "env": {},
            "program": "${file}",
            "cwd": "${fileDirname}",
            "startupQuery": "start",
            "stopOnEntry": true,
            "traceCmds": {
                "continue": [
                    "leap",
                    "l"
                ],
                "stepover": [
                    "skip",
                    "s"
                ],
                "stepinto": [
                    "creep",
                    "c"
                ],
                "stepout": [
                    "up",
                    "u"
                ]
            }
        }
    ]
}